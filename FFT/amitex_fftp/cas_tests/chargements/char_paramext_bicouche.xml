<?xml version="1.0" encoding="UTF-8"?>
<Loading_Output>

<!-- OUPUT QUANTITIES -->
<Output>
    <vtk_StressStrain Strain = "1" Stress = "1"/>
    <!--<vtk_IntVarList numM="1"> -->
    <!--</vtk_IntVarList> -->
    <!--<vtk_IntVarList numM="2"> -->
    <!--</vtk_IntVarList> -->
</Output>
<!-- Initialization of the temperature and the external parameters -->
<InitLoadExt>
  <!-- <T Value="0.01" /> -->
  <Param Index ="1" Value="0.01" />
</InitLoadExt>
<!-- SUCCESSIVE LOADINGS AND OUTPUT TIMES -->
<Loading Tag="1">
