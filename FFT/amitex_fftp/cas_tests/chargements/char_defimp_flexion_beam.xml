<?xml version="1.0" encoding="UTF-8"?>
<Loading_Output>

<!-- OUPUT QUANTITIES -->
<Output>
    <vtk_StressStrain Strain = "1" Stress = "1"/>
    <!--<vtk_IntVarList numM="1"> -->
    <!--</vtk_IntVarList> -->
</Output>

<!-- SUCCESSIVE LOADINGS AND OUTPUT TIMES -->
<Loading Tag="1">
    <Time_Discretization Discretization="Linear" Nincr="1" Tfinal="100"/>
    <Output_vtkList> 1 </Output_vtkList>
    <xx Driving="Strain" Evolution="Linear" Value="0.01"/>
    <yy Driving="Strain" Evolution="Linear" Value="0" />
    <zz Driving="Strain" Evolution="Linear" Value="0" />
    <xy Driving="Strain" Evolution="Linear" Value="0" />
    <xz Driving="Strain" Evolution="Linear" Value="0" />
    <yz Driving="Strain" Evolution="Linear" Value="0" />

    <gradgradU_xx_y      Evolution="Linear" Value="0.001" />    <!-- Applied beam Flexion -->
    <gradgradU_yy_y      Evolution="Linear" Value="-0.0003" /> <!-- Transverse Poisson effect --> 
    <gradgradU_zz_y      Evolution="Linear" Value="-0.0003" /> <!-- Transverse Poisson effect -->
                                                            <!-- Transverse gradients are not prescribed 
                                                                    but influence the iterative algorithm 
                                                                    should be chosen as close as the expected response --> 

</Loading>


</Loading_Output>
